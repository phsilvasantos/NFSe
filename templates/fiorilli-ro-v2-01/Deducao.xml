<nfse:Deducao>
	<nfse:TipoDeducao>{@TipoDeducao}</nfse:TipoDeducao>
	<nfse:DescricaoDeducao>{@DescricaoDeducao}</nfse:DescricaoDeducao>
	<nfse:IdentificacaoDocumentoDeducao>
		{@ifIdentificacaoNfse}
		<nfse:IdentificacaoNfse>
			<nfse:CodigoMunicipioGerador>{@CodigoMunicipioGerador}</nfse:CodigoMunicipioGerador>
			<nfse:NumeroNfse>{@NumeroNfse}</nfse:NumeroNfse>
			<nfse:CodigoVerificacao>{@CodigoVerificacao}</nfse:CodigoVerificacao>
		</nfse:IdentificacaoNfse>
		{@endifIdentificacaoNfse}
		{@ifIdentificacaoNfe}
		<nfse:IdentificacaoNfe>
			<nfse:NumeroNfe>{@NumeroNfe}</nfse:NumeroNfe>
			<nfse:UfNfe>{@UfNfe}</nfse:UfNfe>
			<nfse:ChaveAcessoNfe>{@ChaveAcessoNfe}</nfse:ChaveAcessoNfe>
		</nfse:IdentificacaoNfe>
		{@endifIdentificacaoNfe}
		{@ifOutroDocumento}
		<nfse:OutroDocumento>
			<nfse:IdentificacaoDocumento>{@IdentificacaoDocumento}</nfse:IdentificacaoDocumento>
		</nfse:OutroDocumento>
		{@endifOutroDocumento}
	</nfse:IdentificacaoDocumentoDeducao>
	<nfse:DadosFornecedor>
		{@ifIdentificacaoFornecedor}
		<nfse:IdentificacaoFornecedor>
			<nfse:CpfCnpj>
				{@ifCpf}
				<nfse:Cpf>{@Cpf}</nfse:Cpf>
				{@endifCpf}
				{@ifCnpj}
				<nfse:Cnpj>{@Cnpj}</nfse:Cnpj>
				{@endifCnpj}
			</nfse:CpfCnpj>
		</nfse:IdentificacaoFornecedor>
		{@endifIdentificacaoFornecedor}
		{@ifFornecedorExterior}
		<nfse:FornecedorExterior>
			{@ifNifFornecedor}
			<nfse:NifFornecedor>{@NifFornecedor}</nfse:NifFornecedor>
			{@endifNifFornecedor}
			<nfse:CodigoPais>{@CodigoPais}</nfse:CodigoPais>
		</nfse:FornecedorExterior>
		{@endifFornecedorExterior}
	</nfse:DadosFornecedor>
	<nfse:DataEmissao>{@DataEmissao}</nfse:DataEmissao>
	<nfse:ValorDedutivel>{@ValorDedutivel}</nfse:ValorDedutivel>
	<nfse:ValorUtilizadoDeducao>{@ValorUtilizadoDeducao}</nfse:ValorUtilizadoDeducao>
</nfse:Deducao>